{
  "manifest_version": 3,
  "name": "GoBack Timestamp",
  "description": "Go back to timestamp you've clicked",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["./scripts/main.js"],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "96": "assets/timestamp-rabbit-96.png",
    "128": "assets/timestamp-rabbit-128.png"
  }
}
